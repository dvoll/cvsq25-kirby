{
    "name": "dvll/cvsq25-kirby",
    "description": "CVJM SQ Website",
    "homepage": "https://cvjm-stift-quernheim.de",
    "type": "project",
    "keywords": [
        "kirby",
        "cms",
        "website",
        "newsletter"
    ],
    "authors": [
        {
            "name": "Dario Voll",
            "homepage": "https://daario.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1.2",
        "getkirby/cms": "^4.1",
        "getkirby/cli": "^1.2",
        "arnoson/kirby-vite": "^5.3",
        "tobimori/kirby-seo": "^1.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.52",
        "phpstan/phpstan": "^1.10"
    }
}
